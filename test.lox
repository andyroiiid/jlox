fun hanoi(a, b, c, n) {
    if (n <= 1) {
        print a + " -> " + c;
    } else {
        hanoi(a, c, b, n - 1);
        print a + " -> " + c;
        hanoi(b, a, c, n - 1);
    }
}

print hanoi;
hanoi("A", "B", "C", 3);
